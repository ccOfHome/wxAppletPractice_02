.row{display: flex;flex-direction: row;justify-content: space-around;height: 50rpx;}

.title{width: 33%;text-align: center;padding-bottom: 70rpx;font-size: 32rpx;}

.contain{margin: 15rpx;padding: 10rpx;background-color: white;padding-left: 15rpx;}

.content{margin: 15rpx;padding: 10rpx;background-color: white;padding-left: 15rpx;border-bottom: 2rpx solid grey;padding-bottom: 50rpx;}

.text{font-size: 36rpx;margin-top: 15rpx;display: flex;flex-direction: row;justify-content: space-between;}

.empty{display: flex;flex-direction: column;align-items: center;margin-top: 200rpx;}

.emptyImg{width: 180rpx;height: 220rpx;}